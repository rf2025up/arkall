{
  "name": "arkok-v2-server",
  "version": "2.0.0",
  "description": "ArkOK V2 SaaS Backend Service",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "./scripts/type-safety-check.sh",
    "type-audit": "./scripts/type-definition-auditor.sh",
    "analyze-ts-errors": "ts-node -e \"import { analyzeCompileErrors, generateFixReport } from './src/utils/ts-root-cause-analyzer'; const compileOutput = require('child_process').execSync('npm run build 2>&1', { encoding: 'utf8' }); const analyses = analyzeCompileErrors(compileOutput); console.log(generateFixReport(analyses));\"",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "migrate:legacy": "ts-node prisma/migrate_legacy.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.11.3",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.4",
    "prisma": "^5.7.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "saas",
    "education",
    "management",
    "arkok"
  ],
  "author": "ArkOK Team",
  "license": "MIT"
}
